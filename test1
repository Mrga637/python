n1 = int(input('ye adad bego '))
n2 = int(input('ye adad dige ham begoo '))
hesab = input('mikhay ba adadet che konam entekab kon (+   -   *) ')
if hesab == '+' :
    print(f'khedmt shoma{n1}'+f'{n2}')
elif hesab == '*':
    print(f'khedmat shoma{n1}' * f'{n2}')
elif hesab == '-':
    print(n1-n2)
else :
    print('nmidonm chetrie')
